{
  "name": "uk-shop",
  "version": "1.0.0",
  "description": "This is a e-commerce store which have all the functionalty which are necessery for a ecommerce shop",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
